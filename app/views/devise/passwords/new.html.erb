<%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
  <%= devise_error_messages! %>

  <div class="field form-group">
    <%= f.email_field :email, autofocus: true, :class => "login-form", placeholder: "Username" %>
  </div>

  <div class="actions">
    <%= f.submit "Send me reset password instructions", :class => "dashboard-login" %>
  </div>
<% end %>
<br>